local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Speed And Jump Settings", "DarkTheme")

local Tab = Window:NewTab("Main")

local Section = Tab:NewSection("Speed")

Section:NewSlider("SpeedSlider", "SliderInfo", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
_G.loops = true
while wait(1) do
if _G.loops == true then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
print("Anti Cheat Reset the Speed")
print("Re executing...)
end
end
end)

local Section = Tab:NewSection("Jump")

Section:NewSlider("SliderText", "SliderInfo", 150, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
